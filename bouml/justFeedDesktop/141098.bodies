                });
        connect(boutonsChangerProduit[i],
                &QPushButton::clicked,
                this,
                [this, i]()
                {
                    changerLeProduit(i);
                });
        connect(boutonSuppressionBac[i],
                &QPushButton::clicked,
                this,
                [this, i]()
                {
                    supprimerBac(i);
                });
    }
    connect(boutonAjoutBac, SIGNAL(clicked()), this, SLOT(ajouterBac()));
